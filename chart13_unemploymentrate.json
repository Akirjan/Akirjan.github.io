{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {
      "url": "https://raw.githubusercontent.com/Akirjan/Akirjan.github.io/main/data_ONS-lf2q.json",
      "format": {"type": "json", "property": "years"}
    },
    "title": {
            "text": "LFS: ILO Unemployment rate: UK: All: Aged 16-64: SA",
            "subtitle": "Annual: Source: ONS",
            "subtitleFontStyle": "Arial",
            "anchor": "start",
            "color": "black"
          },
    "width": 300,
    "height": 300,
    "mark": {
      "type": "line",
      "color": "rgb(0,47,167"
    },
    "encoding": {
        "x": {"field": "date", "type": "temporal", "axis": {"grid": false}, "title": ""},
        "y": {"field": "value", "type": "quantitative", "axis": {"grid": false}, "title": "Unemployment rate (%)"}
    }
  }