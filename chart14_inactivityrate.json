{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {
      "url": "https://raw.githubusercontent.com/Akirjan/Akirjan.github.io/main/data_ONS-lf2w.json",
      "format": {"type": "json", "property": "years"}
    },
         "title": {
              "text": "LFS: Economic inactivity rate: UK: All: Aged 50-64: SA",
              "subtitle": "Annual: Source: ONS",
              "subtitleFontStyle": "Arial",
              "anchor": "start",
              "color": "black"
            },
    "width": 300,
    "height": 300,
    "mark": {
      "type": "line",
      "color": "rgb(0,47,167"   
    },
    "encoding": {
        "x": {"field": "date", "type": "temporal", "axis": {"grid": false}, "title": ""},
        "y": {"field": "value", "type": "quantitative", "axis": {"grid": false}, "title": "Inactivity rate (%)"}
    }
  }