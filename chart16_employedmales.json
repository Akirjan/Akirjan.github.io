
{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {
      "url": "https://raw.githubusercontent.com/Akirjan/Akirjan.github.io/main/data_ONS-mgsv.json",
      "format": {"type": "json", "property": "years"}
    },
         "title": {
              "text": "Male employment rate in the UK",
              "subtitle": "Aged 16-64 from 1971-2022: Source: ONS",
              "subtitleFontStyle": "italic",
              "subtitleFontSize":10,
              "anchor": "start",
              "color": "black"
            },
    "width": 200,
    "height": 180,
    "mark": {
      "type": "line",
      "color": "rgb(0,47,167"   
    },
    "encoding": {
        "x": {"field": "date", "type": "temporal", "axis": {"grid": false}, "title": ""},
        "y": {"field": "value", "type": "quantitative", "axis": {"grid": false}, "title": "Inactivity rate (%)"}
    }
  }